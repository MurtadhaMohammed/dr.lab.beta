{
  "name": "lab-beta",
  "version": "2.0.1",
  "homepage": "./",
  "main": "index.js",
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@parcel/watcher": "^2.4.1",
    "antd": "^5.5.0",
    "better-sqlite3": "^11.0.0",
    "caniuse-lite": "^1.0.30001641",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.7",
    "dayjs-plugin-utc": "^0.1.2",
    "dropbox": "^10.34.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.1.7",
    "electron-updater": "^6.2.1",
    "express": "^4.19.2",
    "file-dialog": "^0.0.8",
    "form-data": "^4.0.0",
    "framer-motion": "^11.3.19",
    "get-file-object-from-local-path": "^1.0.2",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-electron-fs-backend": "^3.0.2",
    "jimp": "^0.22.12",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.29",
    "jwt-decode": "^4.0.0",
    "lightningcss": "^1.25.1",
    "nanoid": "^4.0.2",
    "nedb": "^1.8.0",
    "node-machine-id": "^1.1.12",
    "pdf-to-printer": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.11.2",
    "zustand": "^4.3.8"
  },
  "scripts": {
    "start": "concurrently \"npx parcel ./index.html\" \"wait-on http://localhost:1234 && electron index.js .\"",
    "build": "npx electron-packager . Dr.Lab --overwrite --asar=true --platform=win32 --arch=ia32 --ignore=.parcel-cache  --out=build",
    "build-mac": "npx electron-packager . Dr.Lab --overwrite --asar=true --platform=darwin --arch=arm64 --ignore=.parcel-cache --out=build",
    "react-build": "parcel  splash.html index.html  --public-url ./",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "package": "electron-builder",
    "package:win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "buffer": "^5.5.0",
    "concurrently": "^8.0.1",
    "crypto-browserify": "^3.12.0",
    "electron": "^24.3.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.1",
    "events": "^3.3.0",
    "parcel": "^2.8.3",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.39",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.6",
    "tailwindcss-bg-patterns": "^0.3.0",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "sqlite.electron-test.app",
    "productName": "Dr.Lab",
    "extends": null,
    "npmRebuild": false,
    "files": [
      "./index.js",
      "src/**/*",
      "dist/**/*",
      "./initPDF.js",
      "./node_modules/**/*",
      "./Frutiger-normal.js",
      "./splash.html"
    ],
    "win": {
      "icon": "logo.png",
      "target": [
        "nsis"
      ]
    },
    "directories": {
      "app": ".",
      "buildResources": "public",
      "output": "out"
    },
    "extraResources": [
      "./database.sql"
    ]
  }
}
