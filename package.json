{
  "name": "lab-beta",
  "version": "0.1.0",
  "homepage": "./",
  "main": "index.js",
  "dependencies": {
    "@ant-design/icons": "^5.1.1",
    "antd": "^5.5.0",
    "better-sqlite3": "^11.0.0",
    "caniuse-lite": "^1.0.30001641",
    "dayjs": "^1.11.7",
    "dayjs-plugin-utc": "^0.1.2",
    "dropbox": "^10.34.0",
    "electron-is-dev": "^2.0.0",
    "form-data": "^4.0.0",
    "get-file-object-from-local-path": "^1.0.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.29",
    "nanoid": "^4.0.2",
    "nedb": "^1.8.0",
    "node-machine-id": "^1.1.12",
    "pdf-to-printer": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.11.2",
    "zustand": "^4.3.8"
  },
  "scripts": {
    "start": "concurrently \"parcel ./index.html \" \"wait-on http://localhost:1234 && electron ./index.js\"",
    "react-build": "parcel  splash.html index.html  --public-url ./",
    "build": "npx electron-packager . Dr.Lab --overwrite --asar=true --platform=win32 --arch=ia32 --ignore=.parcel-cache  --out=build",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "package-windows": "electron-builder"
  },
  "build": {
    "appId": "com.electron",
    "files": [
      "dist/electron.js",
      "dist/index.html",
      "dist/main.js",
      "dist/images",
      "dist/preload.js",
      "dist/**"
    ],
    "extraResources": [
      {
        "from": "./database/",
        "to": "databases/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "output": "./electron"
    },
    "electronDownload": {
      "cache": "./electron/cache"
    },
    "win": {
      "target": "msi",
      "icon": "./src/assets/images/icon.png"
    },
    "asar": false
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "buffer": "^5.5.0",
    "concurrently": "^8.0.1",
    "crypto-browserify": "^3.12.0",
    "electron": "^24.3.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.1",
    "events": "^3.3.0",
    "kill-port": "^2.0.1",
    "parcel": "^2.8.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "wait-on": "^7.0.1"
  }
}
